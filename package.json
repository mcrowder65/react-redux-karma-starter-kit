{
  "name": "react-redux-karma-starter-kit",
  "version": "1.1.7",
  "description": "React, Redux, Webpack, Babel, Karma, and code coverage all provided for you",
  "main": "index.js",
  "keywords": [
    "react",
    "redux",
    "react router",
    "webpack",
    "karma",
    "istanbul",
    "mocha",
    "chai",
    "react starter kit"
  ],
  "scripts": {
    "install": "sh setup.sh",
    "postinstall": "npm uninstall --save react-redux-karma-starter-kit && npm uninstall --save-dev react-redux-karma-starter-kit",
    "client": "webpack-dev-server",
    "build": "webpack",
    "karma": "rm -rf coverage && karma start",
    "test": "npm prune && npm run linter && npm run karma",
    "linter": "npm run linter-src && npm run linter-test",
    "linter-src": "eslint src --ext .js,.jsx",
    "linter-test": "eslint test --ext .js,.jsx",
    "check": "npm test && npm run build",
    "start": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcrowder65/react-redux-karma-starter-kit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "html-webpack-plugin": "^2.30.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "eslint": "^4.7.2",
    "eslint-config-walmart": "^1.2.4",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.3.0",
    "karma": "^1.7.1",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.3",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.18.2"
  }
}
